@if (ViewBag.msg != null)
{
    <script>alert(@ViewBag.msg)</script>
}
<!DOCTYPE html>
<html lang="en">
<head>
    <title>Login V14</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>

    <link rel="stylesheet" href="~/fonts/icomoon/style.css">

    <link rel="stylesheet" href="~/css/owl.carousel.min.css">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="css/bootstrap.min.css">
    <link rel="icon" type="image/png" href="~/img/icons/favicon.ico" />
    <link rel="stylesheet" type="text/css" href="~/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/css/mainchangepass.css">
    <link rel="stylesheet" type="text/css" href="~/css/styleresetpass.css">
    <!--===============================================================================================-->
</head>
<body>

    <div class="content">
        <div class="container">
            <div class="row" style="text-align:center">
                <div class="col-md-6 contents">
                    <div class="row justify-content-center">
                        <div class="col-md-8">
                            <div class="form-group first">
                                <form method="post" asp-controller="account" asp-action="checkmail">
                                    <label for="username">Enter code</label>
                                    <input type="text" class="form-control" id="username" name="numcode" placeholder="Enter code">
                                    <button type="submit" class="btn btn-info btn-lg">Enter</button>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>